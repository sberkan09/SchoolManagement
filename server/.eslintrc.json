{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "object-curly-newline": "off",
        "camelcase": "off",
        "prefer-destructuring": "off",
        "no-param-reassign": "off",
        "max-len": "off"
    }
}
